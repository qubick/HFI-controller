{
  "name": "three",
  "version": "0.86.0",
  "description": "JavaScript 3D library",
  "main": "build/three.js",
  "repository": "mrdoob/three.js",
  "jsnext:main": "build/three.module.js",
  "module": "build/three.module.js",
  "files": [
    "package.json",
    "bower.json",
    "LICENSE",
    "README.md",
    "build/three.js",
    "build/three.min.js",
    "build/three.module.js",
    "src",
    "examples/js",
    "examples/fonts"
  ],
  "directories": {
    "doc": "docs",
    "example": "examples",
    "test": "test"
  },
  "eslintConfig": {
    "extends": "mdcs"
  },
  "scripts": {
    "build": "rollup -c",
    "build-test": "rollup -c test/rollup.unit.config.js",
    "build-uglify": "rollup -c && uglifyjs build/three.js -cm --preamble \"// threejs.org/license\" > build/three.min.js",
    "build-closure": "rollup -c && java -jar node_modules/google-closure-compiler/compiler.jar --warning_level=VERBOSE --jscomp_off=globalThis --jscomp_off=checkTypes --externs utils/build/externs.js --language_in=ECMASCRIPT5_STRICT --js build/three.js --js_output_file build/three.min.js",
    "dev": "rollup -c -w -m inline",
    "lint": "eslint src",
    "test": "rollup -c test/rollup.unit.config.js -w",
    "editor": "electron ./editor/main.js"
  },
  "keywords": [
    "three",
    "three.js",
    "3d",
    "webgl"
  ],
  "author": "mrdoob",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mrdoob/three.js/issues"
  },
  "homepage": "http://threejs.org/",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-flow": "^6.23.0",
    "electron": "^1.8.4",
    "eslint": "^3.10.1",
    "eslint-config-mdcs": "^4.2.2",
    "google-closure-compiler": "^20160713.0.0",
    "qunitjs": "^2.1.1",
    "rollup": "^0.41.4",
    "rollup-watch": "^3.2.2",
    "uglify-js": "^2.6.0"
  },
  "dependencies": {
    "@jscad/cli": "^1.6.3",
    "@jscad/openjscad": "^1.6.1",
    "array-flatten": "^2.1.1",
    "colors": "^1.2.1",
    "content-disposition": "^0.5.2",
    "content-type": "^1.0.4",
    "craftml": "^2.0.0-alpha",
    "cylon": "^1.3.0",
    "cylon-leapmotion": "^0.21.0",
    "depd": "^1.1.2",
    "express": "^4.16.2",
    "finalhandler": "^1.1.0",
    "http-errors": "^1.6.2",
    "merge-descriptors": "^1.0.1",
    "methods": "^1.1.2",
    "nodemon": "^1.17.2",
    "npm": "^6.13.4",
    "path-to-regexp": "^2.1.0",
    "promise": "^8.0.1",
    "qs": "^6.5.1",
    "react": "^16.2.0",
    "react-dropzone": "^4.0.0",
    "serialport": "^6.0.5",
    "setprototypeof": "^1.1.0",
    "synchronize": "^2.0.0",
    "utils-merge": "^1.0.1",
    "victor": "^1.1.0"
  }
}
